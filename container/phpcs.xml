<?xml version="1.0"?>
<ruleset name="PHP_CodeSniffer">
  <description>PHPCS configuration file.</description>
  <rule ref="PSR12" />
  <arg name="extensions" value="php,inc,phtml" />
  <exclude-pattern>*/migrations/*</exclude-pattern>
  <rule ref="PSR1">
    <exclude name="PSR1.Files.SideEffects" />
  </rule>
</ruleset>